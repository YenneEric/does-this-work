<Window x:Class="View.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:View"
        mc:Ignorable="d"
        Title="MainWindow" Height="800" Width="1200">
    <Grid>
        <local:HomePage x:Name="HomePage" Visibility="Visible" Width="Auto" Height="Auto" />
        <local:InsertData x:Name="InsertData" Visibility="Hidden" Width="Auto" Height="Auto" />
        <local:AddPlayerPage x:Name="AddPlayerPage" Visibility="Hidden" Width="Auto" Height="Auto" />
        <local:MostTouchdowns x:Name="MostTouchdowns" Visibility="Hidden" Width="Auto" Height="Auto" />
        <local:ConfrenceWins x:Name="ConfrenceWins" Visibility="Hidden" Width="Auto" Height="Auto" />
        <local:MostTeamYards x:Name="MostTeamYards" Visibility="Hidden" Width="Auto" Height="Auto" />
        <local:TopScoring x:Name="TopScoring" Visibility="Hidden" Width="Auto" Height="Auto" />
    </Grid>
</Window>
